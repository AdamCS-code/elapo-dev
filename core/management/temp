"rating" integer NOT NULL, "customer_id" char(32) NOT NULL REFERENCES "main_customer" ("id") DEFERRABLE INITIALLY DEFERRED, "order_id" char(32) NOT NULL REFERENCES "order_order" ("id") DEFERRABLE INITIALLY DEFERRED);

